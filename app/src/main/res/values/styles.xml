<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="card">
        <item name="android:layout_margin">@dimen/card_margin</item>
        <item name="android:layout_width">@dimen/card_width</item>
        <item name="android:layout_height">@dimen/card_height</item>
        <item name="android:background">@drawable/back</item>
    </style>

</resources>
